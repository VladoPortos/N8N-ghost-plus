{
  "name": "@vladoportos/n8n-nodes-ghostv2",
  "version": "0.1.4",
  "description": "n8n node for Ghost CMS V2",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "ghost",
    "cms",
    "api"
  ],
  "license": "MIT",
  "homepage": "https://github.com/VladoPortos/N8N-ghost-v2",
  "author": {
    "name": "VladoPortos",
    "email": "vladoportos@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VladoPortos/N8N-ghost-v2.git"
  },
  "main": "dist/nodes/GhostV2/Ghostv2.node.js",
  "scripts": {
    "build": "tsc && gulp build:icons && gulp build:translations",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build && npm run lint"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/GhostV2AdminApi.credentials.js",
      "dist/credentials/GhostV2ContentApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/GhostV2/Ghostv2.node.js"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.24.1",
    "@typescript-eslint/parser": "~5.45.1",
    "eslint": "^8.56.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "gulp": "^4.0.2",
    "prettier": "^2.7.1",
    "typescript": "~4.8.4"
  },
  "peerDependencies": {
    "n8n-core": ">=0.125.0",
    "n8n-workflow": ">=0.107.0"
  }
}
